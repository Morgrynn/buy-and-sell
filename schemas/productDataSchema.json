{
    "type": "object",
    "title": "Product Details",
    "required": [
        "title",
        "description",
        "city",
        "country",
        "price",
        "shipping",
        "pickup",
        "category"
    ],
    "properties": {
        "title": {
            "$id": "#/properties/title",
            "type": "string",
            "title": "The title schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Chair"
            ]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "A black chair. Good condition."
            ]
        },
        "city": {
            "$id": "#/properties/city",
            "type": "string",
            "title": "The city schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Nurmes"
            ]
        },
        "country": {
            "$id": "#/properties/country",
            "type": "string",
            "title": "The country schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Finland"
            ]
        },
        "images": {
            "$id": "#/properties/images",
            "type": "array",
            "title": "The images schema",
            "description": "An explanation about the purpose of this instance.",
            "default": [],
            "examples": [
                [
                    {
                        "originalname": "mando.jpg"
                    },
                    {
                        "originalname": "Morty.png"
                    }
                ]
            ],
            "items": {
                "$id": "#/properties/images/items",
                "anyOf": [
                    {
                        "$id": "#/properties/images/items/anyOf/0",
                        "type": "object",
                        "title": "The first anyOf schema",
                        "description": "An explanation about the purpose of this instance.",
                        "default": {},
                        "examples": [
                            {
                                "originalname": "mando.jpg"
                            }
                        ],
                        "required": [
                            "originalname"
                        ],
                        "properties": {
                            "originalname": {
                                "$id": "#/properties/images/items/anyOf/0/properties/originalname",
                                "type": "string",
                                "title": "The originalname schema",
                                "description": "An explanation about the purpose of this instance.",
                                "default": "",
                                "examples": [
                                    "mando.jpg"
                                ]
                            }
                        },
                        "additionalProperties": false
                    }
                ]
            }
        },
        "price": {
            "$id": "#/properties/price",
            "type": "string",
            "title": "The price schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "13.99"
            ]
        },
        "shipping": {
            "$id": "#/properties/shipping",
            "type": "string",
            "title": "The shipping schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "true"
            ]
        },
        "pickup": {
            "$id": "#/properties/pickup",
            "type": "string",
            "title": "The pickup schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "false"
            ]
        },
        "category": {
            "$id": "#/properties/category",
            "type": "string",
            "title": "The category schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Electronics"
            ]
        }
    },
    "additionalProperties": false
}